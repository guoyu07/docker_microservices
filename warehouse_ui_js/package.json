{
  "name": "warehouse_ui_js",
  "version": "1.0.0",
  "description": "Sample AngularJS app",
  "main": "./src/js/app.js",
  "browserify": {
    "transform": ["browserify-ng-html2js"]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "http-server -a 0.0.0.0 -p 8000 -c-1 ./dist/",
    "clean": "rimraf dist",
    "init": "mkdir -p ./dist",
    "prebuild": "npm run init && cp ./src/index.html ./dist/",
    "build": "browserify ./src/js/app.js -o ./dist/bundle.js",
    "build:minify": "npm run build && uglifyjs ./dist/bundle.js -o ./dist/bundle.js"
  },
  "keywords": [
    "sample",
    "angular"
  ],
  "author": "sergey.maksymenko@gmail.com",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.8",
    "angular-ui-router": "^0.3.1"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "browserify-ng-html2js": "^1.3.0",
    "rimraf": "^2.5.4",
    "uglifyjs": "^2.4.10"
  }
}

{
  "name": "warehouse_ui_js",
  "version": "1.0.0",
  "description": "Sample AngularJS app",
  "main": "./app/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "http-server -a 0.0.0.0 -p 8000 -c-1 ./dist/",
    "clean": "rimraf dist",
    "prebundle": "mkdir -p ./dist/js  && cp ./src/index.html ./dist/ && cp ./src/js/*.html ./dist/",
    "bundle": "browserify ./src/js/app.js -o ./dist/js/bundle.js",
    "package": "npm run bundle && uglifyjs ./dist/js/bundle.js -mc -o ./dist/js/bundle.js"
  },
  "keywords": [
    "sample",
    "angular"
  ],
  "author": "sergey.maksymenko@gmail.com",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.8",
    "angular-ui-router": "^0.3.1"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "rimraf": "^2.5.4",
    "uglifyjs": "^2.4.10"
  }
}

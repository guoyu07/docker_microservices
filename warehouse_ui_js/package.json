{
  "name": "warehouse_ui_js",
  "version": "1.0.0",
  "description": "Sample AngularJS app",
  "main": "./src/js/app.js",
  "browserify": {
    "transform": [
      "browserify-ng-html2js"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "http-server -a 0.0.0.0 -p 8000 -c-1 ./dist/",
    "server-dev": "webpack-dev-server --history-api-fallback --inline --progress",
    "clean": "rimraf dist",
    "init": "npm run clean && mkdir -p ./dist",
    "prebuild": "npm run init && cp ./src/index.html ./dist/",
    "build": "webpack --bail --progress --profile"
  },
  "keywords": [
    "sample",
    "angular"
  ],
  "author": "sergey.maksymenko@gmail.com",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.8",
    "angular-ui-router": "^0.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "webpack": "^1.14.0"
  }
}
